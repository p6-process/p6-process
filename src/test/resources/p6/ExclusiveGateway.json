{
  "metadata": {
    "processId": "ExclusiveProcessTest",
    "processVersion": "1.0.0",
    "application": "p6-example",
    "module": "p6-example",
    "processUrl": "http://p6-example:8080"
  },
  "process": [
    {
      "nodeType": "START_EVENT",
      "name": "start",
      "sequence": {
        "from": [
        ],
        "to": [
          "gateway1"
        ]
      }
    },
    {
      "nodeType": "EXCLUSIVE_GATEWAY",
      "name": "gateway1",
      "sequence": {
        "from": [
          "start"
        ],
        "to": [
          "test1",
          "test2"
        ]
      },
      "defaultSequence": "test2",
      "sequenceFlow": "DIVERGING",
      "condition" : {
        "test1": "count < 10 && result",
        "test2": "count >= 10 && result"
      }
    },
    {
      "nodeType": "SERVICE_TASK",
      "name": "test1",
      "sequence": {
        "from": [
          "gateway1"
        ],
        "to": [
          "gateway2"
        ]
      }
    },
    {
      "nodeType": "SERVICE_TASK",
      "name": "test2",
      "sequence": {
        "from": [
          "gateway1"
        ],
        "to": [
          "gateway2"
        ]
      }
    },
    {
      "nodeType": "EXCLUSIVE_GATEWAY",
      "name": "gateway2",
      "sequence": {
        "from": [
          "test1",
          "test2"
        ],
        "to": [
          "end"
        ]
      },
      "sequenceFlow": "CONVERGING"
    },
    {
      "nodeType": "END_EVENT",
      "name": "end",
      "sequence": {
        "from": [
          "gateway2"
        ],
        "to": [
        ]
      }
    }
  ]
}
